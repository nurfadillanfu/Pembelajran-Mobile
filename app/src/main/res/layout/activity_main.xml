<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/r1"
        android:layout_width="match_parent"
        android:layout_height="222dp"
        android:background="@color/red"
        android:padding="14dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="16dp">


        <EditText
            android:id="@+id/et1"
            android:layout_width="300dp"
            android:layout_height="45dp"
            android:background="@color/white"
            android:drawableLeft="@drawable/ic_baseline_youtube_searched_for_24"
            android:drawablePadding="5dp"
            android:hint="@string/etawal"

            android:inputType="text"
            android:paddingHorizontal="10dp"
            app:layout_constraintLeft_toLeftOf="@id/r1"
            app:layout_constraintTop_toTopOf="@id/r1" />

        <ImageView
            android:id="@+id/i1"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@drawable/ic_baseline_notifications_active_24"
            app:layout_constraintBottom_toBottomOf="@id/et1"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/et1" />

        <EditText
            android:id="@+id/et2"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:layout_below="@id/et1"
            android:layout_marginTop="16dp"
            android:background="@color/white"
            android:inputType="text"
            android:paddingHorizontal="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/et1" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/r3"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_below="@id/r1"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="16dp"
        android:background="@color/pink"
        android:paddingHorizontal="10dp"
        app:layout_constraintLeft_toLeftOf="@id/r2"
        app:layout_constraintTop_toBottomOf="@id/r1">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/r2"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_below="@id/r1"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:background="@color/pink"
        android:paddingHorizontal="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/r1">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/r4"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_below="@id/r1"
        android:layout_marginLeft="216dp"
        android:layout_marginTop="16dp"
        android:background="@color/pink"
        android:paddingHorizontal="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/r1">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/r5"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_below="@id/r1"
        android:layout_marginLeft="232dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="28dp"
        android:background="@color/pink"
        android:paddingHorizontal="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/r1">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/r6"
        android:layout_width="330dp"
        android:layout_height="120dp"
        android:layout_below="@id/r1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:background="@color/ungu"
        android:paddingHorizontal="10dp"
        app:layout_constraintLeft_toLeftOf="@id/r2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/r2">


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/r7"
        android:layout_width="90dp"
        android:layout_height="80dp"
        android:layout_below="@id/r1"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="28dp"
        android:background="@color/ungu"
        android:paddingHorizontal="10dp"
        app:layout_constraintLeft_toLeftOf="@id/r2"
        app:layout_constraintTop_toBottomOf="@id/r6">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/r8"
        android:layout_width="90dp"
        android:layout_height="80dp"
        android:layout_below="@id/r1"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="28dp"
        android:background="@color/ungu"
        android:paddingHorizontal="10dp"
        app:layout_constraintLeft_toLeftOf="@id/r2"
        app:layout_constraintTop_toBottomOf="@id/r6">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/r9"
        android:layout_width="90dp"
        android:layout_height="80dp"
        android:layout_below="@id/r1"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="28dp"
        android:background="@color/ungu"
        android:paddingHorizontal="10dp"
        app:layout_constraintLeft_toLeftOf="@id/r2"
        app:layout_constraintTop_toBottomOf="@id/r6">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/r10"
        android:layout_width="90dp"
        android:layout_height="80dp"
        android:layout_below="@id/r1"
        android:layout_marginLeft="240dp"
        android:layout_marginTop="28dp"
        android:background="@color/ungu"
        android:paddingHorizontal="10dp"
        app:layout_constraintLeft_toLeftOf="@id/r2"
        app:layout_constraintTop_toBottomOf="@id/r6">

    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/r11"
        android:layout_width="90dp"
        android:layout_height="80dp"
        android:layout_below="@id/r1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="136dp"
        android:background="@color/pink"
        android:paddingHorizontal="10dp"
        app:layout_constraintLeft_toLeftOf="@id/r2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/r6">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/r12"
        android:layout_width="90dp"
        android:layout_height="80dp"
        android:layout_below="@id/r1"
        android:layout_marginStart="116dp"
        android:layout_marginTop="136dp"
        android:background="@color/pink"
        android:paddingHorizontal="10dp"
        app:layout_constraintLeft_toLeftOf="@id/r2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/r6">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/r13"
        android:layout_width="90dp"
        android:layout_height="80dp"
        android:layout_below="@id/r1"
        android:layout_marginStart="216dp"
        android:layout_marginTop="136dp"
        android:background="@color/pink"
        android:paddingHorizontal="10dp"
        app:layout_constraintLeft_toLeftOf="@id/r2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/r6">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/r14"
        android:layout_width="90dp"
        android:layout_height="80dp"
        android:layout_below="@id/r1"
        android:layout_marginLeft="260dp"
        android:layout_marginTop="136dp"
        android:background="@color/pink"
        android:paddingHorizontal="10dp"
        app:layout_constraintLeft_toLeftOf="@id/r2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/r6">

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>